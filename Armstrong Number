#include<stdio.h>
#include<math.h>
int main()
{
    int a,b,c=0,d,p=0,e;
    printf("Enter the number\n");
    scanf("%d",&a);
    b=a;
    while(a>0)
    {
        a=a/10;
        c++;
        
    }
    e=b;
    while(b>0)
    {
        d=b%10;
        p+=pow(d,c);
        b/=10;
    }
    if(e==p)
    printf("Entered no. is armstrong");
    else
     printf("Entered no. is not armstrong");
    return 0;
}
