#include <stdio.h>
int main()
{
    char a;
    printf("Enter a letter ");
    scanf("%c",&a);
    if(a=='A'||a=='E'||a=='I'||a=='O'||a=='U'||a=='a'||a=='e'||a=='i'||a=='o'||a=='u')
    {
        printf("You entered a vowel");
    }
    else
    {
        printf("You entered a consonant");

    }
return 0;
}
