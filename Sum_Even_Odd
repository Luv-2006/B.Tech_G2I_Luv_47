#include<stdio.h>
#include<math.h>
int main()
{
    int n;
    printf("Enter the number range ");
    scanf("%d",&n);
    int e=0,o=0;
    for(int i=1;i<=n;i++)
    {
        if(i%2==0)
       e+=i;
       else
       o+=i;
    }
    printf("Sum of Even numbers is %d\n",e);
    printf("Sum of Odd numbers is %d",o);
    return 0;
}
