#include<stdio.h>
#include<math.h>
int main()
{
    int n,r,c=0,b;
    printf("Enter the number  ");
    scanf("%d",&n);
   b=n;
    while(n>0)
    {
        r=n%10;
        c=c*10+r;
        n=n/10;
        
    }
    
    if(c==b)
    printf("Entered no. is Plaindrome");
    else
    printf("Entered no. is not Plaindrome");
    return 0;
}
