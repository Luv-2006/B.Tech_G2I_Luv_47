#include<stdio.h>
int main()
{
    char a;
    printf("Enter a Character ");
    scanf("%c",&a);
    if(a>='A' && a<='Z'||a>='a' && a<='z')
    {
        printf("You entered a Aplhabet");
        
    }
    else if(a>='0' && a<='9')
    {
        printf("You enterd a digit");
    }
    else
    {
        printf("You entered a special character");
    }
    return 0;
}
